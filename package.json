{
  "name": "@jacekpietal/bouncer.js",
  "version": "2.23.0",
  "description": "static files server + plugins on same instance (üêª)",
  "main": "index.js",
  "repository": "git@github.com:Prozi/bouncer.js.git",
  "author": "Jacek Pietal <prozi85@gmail.com>",
  "license": "MIT",
  "typings": "./bouncer-js.d.ts",
  "bin": {
    "bouncer.js": "./bin/index.js"
  },
  "private": false,
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "test": "jest",
    "start": "npm run demo",
    "demo": "node ./bin/index.js demo",
    "docs": "docma",
    "docs:patch": "sed -i -E 's/id=(.*)--+/id=\\1-/' docs/content/*.html",
    "docs:show": "docma serve",
    "lint": "eslint ./*.js --fix",
    "prettify": "prettier . --write",
    "prepare:npm": "rm -rf node_modules && npm i && npm audit fix",
    "prepare:yarn": "rm -rf node_modules && yarn",
    "groom": "npm run prepare:npm && npm run prepare:yarn",
    "precommit": "npm run docs && npm run lint && npm run prettify"
  },
  "dependencies": {
    "latermom": "^1.0.4",
    "mime-types": "^2.1.30",
    "sillyname": "^0.1.0",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v19.2.0",
    "wanakana": "^4.0.2"
  },
  "homepage": "https://prozi.github.io/bouncer.js/",
  "devDependencies": {
    "@jacekpietal/docma": "^3.3.3",
    "dustjs-linkedin": "^2.7.5",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4",
    "ws": "^7.4.5"
  },
  "peerDependencies": {
    "@angular/core": "^11.2.11"
  },
  "keywords": [
    "nodejs",
    "chat",
    "angular",
    "frontend",
    "backend",
    "websockets",
    "socket-io",
    "socket-io-client",
    "javascript-library",
    "http-server",
    "handshake",
    "static-server",
    "angular-service",
    "uws",
    "uwebsockets",
    "rooms",
    "static-server-nodejs",
    "micro-websockets"
  ],
  "resolutions": {
    "ecstatic": "^4.1.4",
    "clean-css": "^4.1.11",
    "braces": "^2.3.1"
  }
}
